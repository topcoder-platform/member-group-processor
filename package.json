{
  "name": "tc-community-processor",
  "version": "1.0.0",
  "description": "Topcoder - Community Processor",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && nyc --reporter=html --reporter=text mocha test/unit/test.js --timeout 20000 --exit",
    "e2e": "npm run lint && nyc --reporter=html --reporter=text mocha test/e2e/test.js --timeout 20000 --exit"
  },
  "author": "TCSCODER",
  "license": "none",
  "devDependencies": {
    "should": "^13.2.3",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "http-json-response": "^1.0.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "co": "^4.6.0",
    "config": "^1.21.0",
    "get-parameter-names": "^0.3.0",
    "joi": "^9.0.4",
    "lodash": "^4.17.19",
    "no-kafka": "^3.2.4",
    "tc-core-library-js": "appirio-tech/tc-core-library-js.git#v2.6",
    "topcoder-healthcheck-dropin": "^1.0.3",
    "winston": "^2.2.0"
  },
  "engines": {
    "node": "8.x"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "nyc": {
    "exclude": [
      "config/default.js",
      "config/test.js",
      "test/*.js",
      "test/common/*.js",
      "test/unit/*.js",
      "test/e2e/*.js"
    ]
  }
}
